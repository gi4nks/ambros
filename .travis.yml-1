language: go
install: true
go:
  - 1.6.3
  - 1.7.1
  - tip
env:
  global:
    - GOARCH=amd64
    - GO_FOR_RELEASE=1.7.1

env:
    - GIMME_OS=linux GIMME_ARCH=amd64
    - GIMME_OS=darwin GIMME_ARCH=amd64
    - GIMME_OS=windows GIMME_ARCH=amd64

install:
    - go get -d -v ./...

script:
    - go build -v ./...
